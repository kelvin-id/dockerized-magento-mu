{
  "name": "Ludaro/dockerized-magento2ce",
  "description": "Dockerized Magento CE 2.0.2",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/Ludaro/dockerized-magento2ce",
  "require": {
    "magento/magento-composer-installer": "0.1.6",
    "magento/community-edition": "2.0.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ],
  "extra": {
    "magento-root-dir": "web",
    "auto-append-gitignore": true
  },
  "config": {
    "discard-changes": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
    ],
    "post-update-cmd": [
    ]
  }
}
